<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
<shape android:shape="rectangle">
    <gradient android:startColor="#d600b2ee"
        android:endColor="#ca289397"
        android:angle="270" />
    <corners android:radius="10dp" />
    <stroke android:width="8px" android:color="#90171b1b" />
</shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#f5289397"
                android:endColor="#e9007bee"
                android:angle="270" />
            <corners android:radius="10dp" />
            <stroke android:width="5px" android:color="#90171b1b" />
        </shape>
    </item>
</selector>